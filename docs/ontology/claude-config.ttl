@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

# Namespace declarations
@prefix cnv: <https://clap-noun-verb.io/ontology/> .
@prefix sparc: <https://sparc.io/ontology/> .
@prefix agents: <https://claude-flow.io/agents/> .
@prefix config: <https://claude-flow.io/config/> .
@prefix rules: <https://claude-flow.io/rules/> .
@prefix methods: <https://claude-flow.io/methods/> .

# ============================================================================
# ONTOLOGY METADATA
# ============================================================================

cnv:ClaudeConfigOntology a owl:Ontology ;
    rdfs:label "Claude Code Configuration Ontology"@en ;
    rdfs:comment "Comprehensive ontology for CLAUDE.md configuration systems, SPARC methodology, and agent-based development."@en ;
    owl:versionInfo "1.0.0" ;
    owl:versionIRI <https://clap-noun-verb.io/ontology/1.0.0/> .

# ============================================================================
# CORE CLASSES - Project Identity
# ============================================================================

cnv:Project a owl:Class ;
    rdfs:label "Project"@en ;
    rdfs:comment "A software project with specific configuration and methodology."@en .

cnv:ProjectIdentity a owl:Class ;
    rdfs:subClassOf cnv:Project ;
    rdfs:label "Project Identity"@en ;
    rdfs:comment "Core identity properties of a project including language, architecture, and principles."@en .

cnv:Language a owl:Class ;
    rdfs:label "Programming Language"@en ;
    rdfs:comment "Primary programming language used in the project."@en .

cnv:Architecture a owl:Class ;
    rdfs:label "Architecture"@en ;
    rdfs:comment "Software architecture pattern (e.g., binary crate, microservices)."@en .

cnv:Principle a owl:Class ;
    rdfs:label "Core Principle"@en ;
    rdfs:comment "Fundamental design principle (e.g., type-first thinking, zero-cost abstractions)."@en .

# ============================================================================
# METHODOLOGY CLASSES - SPARC & TDD
# ============================================================================

sparc:Methodology a owl:Class ;
    rdfs:label "Development Methodology"@en ;
    rdfs:comment "Structured development methodology (SPARC, TDD, DfLSS)."@en .

sparc:SPARCPhase a owl:Class ;
    rdfs:subClassOf sparc:Methodology ;
    rdfs:label "SPARC Phase"@en ;
    rdfs:comment "A phase in the SPARC methodology workflow."@en .

sparc:Specification a owl:Class ;
    rdfs:subClassOf sparc:SPARCPhase ;
    rdfs:label "Specification"@en ;
    rdfs:comment "Requirements analysis and specification phase."@en .

sparc:Pseudocode a owl:Class ;
    rdfs:subClassOf sparc:SPARCPhase ;
    rdfs:label "Pseudocode"@en ;
    rdfs:comment "Algorithm design and pseudocode phase."@en .

sparc:Architecture a owl:Class ;
    rdfs:subClassOf sparc:SPARCPhase ;
    rdfs:label "Architecture"@en ;
    rdfs:comment "System design and architecture phase."@en .

sparc:Refinement a owl:Class ;
    rdfs:subClassOf sparc:SPARCPhase ;
    rdfs:label "Refinement"@en ;
    rdfs:comment "TDD implementation and refinement phase."@en .

sparc:Completion a owl:Class ;
    rdfs:subClassOf sparc:SPARCPhase ;
    rdfs:label "Completion"@en ;
    rdfs:comment "Integration and completion phase."@en .

methods:ChicagoTDD a owl:Class ;
    rdfs:subClassOf sparc:Methodology ;
    rdfs:label "Chicago TDD"@en ;
    rdfs:comment "State-based testing with real collaborators and behavior verification."@en .

methods:AAAPattern a owl:Class ;
    rdfs:label "Arrange-Act-Assert Pattern"@en ;
    rdfs:comment "Test structure pattern: Arrange inputs, Act on system, Assert outputs."@en .

methods:AndonSignal a owl:Class ;
    rdfs:label "Andon Signal"@en ;
    rdfs:comment "Visual problem indicator that requires stopping work to fix root cause."@en .

methods:AndonCritical a owl:Class ;
    rdfs:subClassOf methods:AndonSignal ;
    rdfs:label "Critical Andon Signal"@en ;
    rdfs:comment "Red signal requiring immediate stop: compiler errors, test failures."@en .

methods:AndonHigh a owl:Class ;
    rdfs:subClassOf methods:AndonSignal ;
    rdfs:label "High Andon Signal"@en ;
    rdfs:comment "Yellow signal requiring attention: compiler warnings, linting errors."@en .

# ============================================================================
# AGENT CLASSES
# ============================================================================

agents:Agent a owl:Class ;
    rdfs:label "Agent"@en ;
    rdfs:comment "An AI agent with specific capabilities and role."@en .

agents:AgentTier a owl:Class ;
    rdfs:label "Agent Tier"@en ;
    rdfs:comment "Classification tier for agent specialization level."@en .

agents:HyperAdvancedAgent a owl:Class ;
    rdfs:subClassOf agents:Agent ;
    rdfs:label "Hyper-Advanced Agent"@en ;
    rdfs:comment "Elite agent with specialized domain expertise (5x output quality)."@en .

agents:CoreAgent a owl:Class ;
    rdfs:subClassOf agents:Agent ;
    rdfs:label "Core Agent"@en ;
    rdfs:comment "Basic agent for simple, straightforward tasks."@en .

agents:Capability a owl:Class ;
    rdfs:label "Agent Capability"@en ;
    rdfs:comment "A specific capability or skill an agent possesses."@en .

agents:UseCase a owl:Class ;
    rdfs:label "Use Case"@en ;
    rdfs:comment "Scenario where a specific agent type should be used."@en .

# ============================================================================
# RULE & CONSTRAINT CLASSES
# ============================================================================

rules:Rule a owl:Class ;
    rdfs:label "Configuration Rule"@en ;
    rdfs:comment "A mandatory rule that must be followed in the project."@en .

rules:AbsoluteRule a owl:Class ;
    rdfs:subClassOf rules:Rule ;
    rdfs:label "Absolute Rule"@en ;
    rdfs:comment "Non-negotiable rule that must always be followed."@en .

rules:ProhibitedPattern a owl:Class ;
    rdfs:subClassOf rules:Rule ;
    rdfs:label "Prohibited Pattern"@en ;
    rdfs:comment "Anti-pattern or practice that is explicitly forbidden."@en .

rules:FileOrganization a owl:Class ;
    rdfs:subClassOf rules:Rule ;
    rdfs:label "File Organization Rule"@en ;
    rdfs:comment "Rule governing where files should be saved."@en .

rules:ConcurrencyRule a owl:Class ;
    rdfs:subClassOf rules:Rule ;
    rdfs:label "Concurrency Rule"@en ;
    rdfs:comment "Rule requiring parallel/concurrent execution patterns."@en .

# ============================================================================
# BUILD & EXECUTION CLASSES
# ============================================================================

config:BuildCommand a owl:Class ;
    rdfs:label "Build Command"@en ;
    rdfs:comment "A command used for building, testing, or validating the project."@en .

config:CargoMakeCommand a owl:Class ;
    rdfs:subClassOf config:BuildCommand ;
    rdfs:label "Cargo Make Command"@en ;
    rdfs:comment "Command executed via cargo make (never direct cargo)."@en .

config:PerformanceSLO a owl:Class ;
    rdfs:label "Performance Service Level Objective"@en ;
    rdfs:comment "Quantitative performance target that must be met."@en .

config:TestCategory a owl:Class ;
    rdfs:label "Test Category"@en ;
    rdfs:comment "Classification of test type (unit, integration, property, snapshot)."@en .

# ============================================================================
# PROPERTIES - Object Properties
# ============================================================================

cnv:hasLanguage a owl:ObjectProperty ;
    rdfs:domain cnv:ProjectIdentity ;
    rdfs:range cnv:Language ;
    rdfs:label "has language"@en .

cnv:hasArchitecture a owl:ObjectProperty ;
    rdfs:domain cnv:ProjectIdentity ;
    rdfs:range cnv:Architecture ;
    rdfs:label "has architecture"@en .

cnv:followsPrinciple a owl:ObjectProperty ;
    rdfs:domain cnv:Project ;
    rdfs:range cnv:Principle ;
    rdfs:label "follows principle"@en .

cnv:followsMethodology a owl:ObjectProperty ;
    rdfs:domain cnv:Project ;
    rdfs:range sparc:Methodology ;
    rdfs:label "follows methodology"@en .

cnv:hasPhase a owl:ObjectProperty ;
    rdfs:domain sparc:Methodology ;
    rdfs:range sparc:SPARCPhase ;
    rdfs:label "has phase"@en .

cnv:hasAgent a owl:ObjectProperty ;
    rdfs:domain cnv:Project ;
    rdfs:range agents:Agent ;
    rdfs:label "has agent"@en .

agents:hasCapability a owl:ObjectProperty ;
    rdfs:domain agents:Agent ;
    rdfs:range agents:Capability ;
    rdfs:label "has capability"@en .

agents:hasTier a owl:ObjectProperty ;
    rdfs:domain agents:Agent ;
    rdfs:range agents:AgentTier ;
    rdfs:label "has tier"@en .

agents:hasUseCase a owl:ObjectProperty ;
    rdfs:domain agents:Agent ;
    rdfs:range agents:UseCase ;
    rdfs:label "has use case"@en .

cnv:enforcesRule a owl:ObjectProperty ;
    rdfs:domain cnv:Project ;
    rdfs:range rules:Rule ;
    rdfs:label "enforces rule"@en .

cnv:prohibitsPattern a owl:ObjectProperty ;
    rdfs:domain cnv:Project ;
    rdfs:range rules:ProhibitedPattern ;
    rdfs:label "prohibits pattern"@en .

cnv:hasBuildCommand a owl:ObjectProperty ;
    rdfs:domain cnv:Project ;
    rdfs:range config:BuildCommand ;
    rdfs:label "has build command"@en .

cnv:hasSLO a owl:ObjectProperty ;
    rdfs:domain cnv:Project ;
    rdfs:range config:PerformanceSLO ;
    rdfs:label "has SLO"@en .

cnv:hasTestCategory a owl:ObjectProperty ;
    rdfs:domain cnv:Project ;
    rdfs:range config:TestCategory ;
    rdfs:label "has test category"@en .

methods:triggersAndonSignal a owl:ObjectProperty ;
    rdfs:domain config:BuildCommand ;
    rdfs:range methods:AndonSignal ;
    rdfs:label "triggers Andon signal"@en .

# ============================================================================
# PROPERTIES - Data Properties
# ============================================================================

cnv:projectName a owl:DatatypeProperty ;
    rdfs:domain cnv:Project ;
    rdfs:range xsd:string ;
    rdfs:label "project name"@en .

cnv:description a owl:DatatypeProperty ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string ;
    rdfs:label "description"@en .

cnv:priority a owl:DatatypeProperty ;
    rdfs:domain agents:Agent ;
    rdfs:range xsd:integer ;
    rdfs:label "priority"@en ;
    rdfs:comment "Agent priority level (1=highest, 10=lowest)."@en .

cnv:mandatory a owl:DatatypeProperty ;
    rdfs:domain rules:Rule ;
    rdfs:range xsd:boolean ;
    rdfs:label "mandatory"@en .

config:commandString a owl:DatatypeProperty ;
    rdfs:domain config:BuildCommand ;
    rdfs:range xsd:string ;
    rdfs:label "command string"@en .

config:timeoutSeconds a owl:DatatypeProperty ;
    rdfs:domain config:BuildCommand ;
    rdfs:range xsd:integer ;
    rdfs:label "timeout seconds"@en .

config:targetValue a owl:DatatypeProperty ;
    rdfs:domain config:PerformanceSLO ;
    rdfs:range xsd:string ;
    rdfs:label "target value"@en .

config:metric a owl:DatatypeProperty ;
    rdfs:domain config:PerformanceSLO ;
    rdfs:range xsd:string ;
    rdfs:label "metric"@en .

agents:agentType a owl:DatatypeProperty ;
    rdfs:domain agents:Agent ;
    rdfs:range xsd:string ;
    rdfs:label "agent type"@en .

# ============================================================================
# AGENT INSTANCES - Hyper-Advanced Agents (Priority)
# ============================================================================

agents:ProductionValidator a agents:HyperAdvancedAgent ;
    rdfs:label "Production Validator"@en ;
    agents:agentType "production-validator" ;
    cnv:priority 1 ;
    cnv:description "Production readiness validation: Docker, OTEL, dependencies, SLO compliance."@en ;
    agents:hasCapability agents:Cap_ValidationExpertise, agents:Cap_InfrastructureKnowledge, agents:Cap_ComplianceChecking ;
    agents:hasUseCase agents:UC_ProductionReadiness, agents:UC_DependencyValidation, agents:UC_ReleaseValidation .

agents:CodeAnalyzer a agents:HyperAdvancedAgent ;
    rdfs:label "Code Analyzer"@en ;
    agents:agentType "code-analyzer" ;
    cnv:priority 1 ;
    cnv:description "Advanced code quality analysis: type safety, technical debt, architecture assessment."@en ;
    agents:hasCapability agents:Cap_StaticAnalysis, agents:Cap_ArchitectureReview, agents:Cap_DebtDetection ;
    agents:hasUseCase agents:UC_CodeReview, agents:UC_QualityAssessment, agents:UC_ArchitectureReview .

agents:SystemArchitect a agents:HyperAdvancedAgent ;
    rdfs:label "System Architect"@en ;
    agents:agentType "system-architect" ;
    cnv:priority 1 ;
    cnv:description "System architecture design: API design, integration patterns, architectural decisions."@en ;
    agents:hasCapability agents:Cap_APIDesign, agents:Cap_PatternSelection, agents:Cap_TypeLevelDesign ;
    agents:hasUseCase agents:UC_SystemDesign, agents:UC_APIDesign, agents:UC_ArchitecturalDecisions .

agents:PerformanceBenchmarker a agents:HyperAdvancedAgent ;
    rdfs:label "Performance Benchmarker"@en ;
    agents:agentType "performance-benchmarker" ;
    cnv:priority 1 ;
    cnv:description "Performance measurement & optimization: benchmarking, bottleneck identification, SLO verification."@en ;
    agents:hasCapability agents:Cap_Benchmarking, agents:Cap_Profiling, agents:Cap_OptimizationStrategies ;
    agents:hasUseCase agents:UC_PerformanceAnalysis, agents:UC_BottleneckDetection, agents:UC_SLOVerification .

agents:BackendDev a agents:HyperAdvancedAgent ;
    rdfs:label "Backend Developer"@en ;
    agents:agentType "backend-dev" ;
    cnv:priority 1 ;
    cnv:description "Backend implementation: Docker, containers, APIs, databases, infrastructure code."@en ;
    agents:hasCapability agents:Cap_DockerExpertise, agents:Cap_APIImplementation, agents:Cap_DatabaseDesign ;
    agents:hasUseCase agents:UC_DockerSetup, agents:UC_APIImplementation, agents:UC_InfrastructureCode .

agents:TaskOrchestrator a agents:HyperAdvancedAgent ;
    rdfs:label "Task Orchestrator"@en ;
    agents:agentType "task-orchestrator" ;
    cnv:priority 1 ;
    cnv:description "Complex workflow orchestration: multi-phase workflows, coordination, dependency management."@en ;
    agents:hasCapability agents:Cap_WorkflowDesign, agents:Cap_DependencyManagement, agents:Cap_Coordination ;
    agents:hasUseCase agents:UC_ComplexWorkflows, agents:UC_MultiPhaseCoordination, agents:UC_DependencyOrchestration .

