# ============================================================================
# RULE INSTANCES
# ============================================================================

rules:AlwaysCargoMake a rules:AbsoluteRule ;
    rdfs:label "Always Use Cargo Make"@en ;
    cnv:mandatory true ;
    cnv:description "NEVER USE DIRECT CARGO COMMANDS - ALWAYS USE cargo make. This is non-negotiable."@en .

rules:NeverSaveToRoot a rules:AbsoluteRule ;
    rdfs:label "Never Save to Root Folder"@en ;
    cnv:mandatory true ;
    cnv:description "NEVER save working files, text/mds and tests to the root folder. Always organize in subdirectories."@en .

rules:ConcurrentExecution a rules:ConcurrencyRule ;
    rdfs:label "Concurrent Execution Required"@en ;
    cnv:mandatory true ;
    cnv:description "ALL operations MUST be concurrent/parallel in a single message."@en .

rules:BatchTodos a rules:ConcurrencyRule ;
    rdfs:label "Batch All Todos"@en ;
    cnv:mandatory true ;
    cnv:description "TodoWrite ALWAYS batches ALL todos in ONE call (10+ todos minimum)."@en .

rules:StopOnAndon a rules:AbsoluteRule ;
    rdfs:label "Stop on Andon Signal"@en ;
    cnv:mandatory true ;
    cnv:description "NEVER IGNORE ANDON SIGNALS. Stop the line when compiler errors, test failures, or warnings appear."@en .

rules:NoUnwrap a rules:ProhibitedPattern ;
    rdfs:label "No unwrap() in Production"@en ;
    cnv:mandatory true ;
    cnv:description "No unwrap()/expect() in production code. Use Result<T, E> error handling."@en .

rules:NoTODO a rules:ProhibitedPattern ;
    rdfs:label "No TODO Comments"@en ;
    cnv:mandatory true ;
    cnv:description "No TODO comments except FUTURE: prefix for documented future enhancements."@en .

rules:NoPlaceholders a rules:ProhibitedPattern ;
    rdfs:label "No Placeholders"@en ;
    cnv:mandatory true ;
    cnv:description "No placeholders - No 'In production, this would...' comments."@en .

rules:ChicagoTDDMandatory a rules:AbsoluteRule ;
    rdfs:label "Chicago TDD Mandatory"@en ;
    cnv:mandatory true ;
    cnv:description "Must use state-based testing, real collaborators, AAA pattern, behavior verification."@en .

# ============================================================================
# BUILD COMMAND INSTANCES
# ============================================================================

config:CargoMakeCheck a config:CargoMakeCommand ;
    rdfs:label "cargo make check"@en ;
    config:commandString "cargo make check" ;
    config:timeoutSeconds 5 ;
    cnv:description "Quick compilation check - detects compiler errors (Andon signal)."@en ;
    methods:triggersAndonSignal methods:CompilerErrorSignal .

config:CargoMakeTest a config:CargoMakeCommand ;
    rdfs:label "cargo make test"@en ;
    config:commandString "cargo make test" ;
    config:timeoutSeconds 40 ;
    cnv:description "All tests (unit + integration) - detects test failures (Andon signal)."@en ;
    methods:triggersAndonSignal methods:TestFailureSignal .

config:CargoMakeLint a config:CargoMakeCommand ;
    rdfs:label "cargo make lint"@en ;
    config:commandString "cargo make lint" ;
    config:timeoutSeconds 10 ;
    cnv:description "Clippy linting - detects code quality issues (Andon signal)."@en ;
    methods:triggersAndonSignal methods:LintingErrorSignal .

config:CargoMakeSLOCheck a config:CargoMakeCommand ;
    rdfs:label "cargo make slo-check"@en ;
    config:commandString "cargo make slo-check" ;
    config:timeoutSeconds 30 ;
    cnv:description "Verify performance SLOs are met."@en .

config:CargoMakeBench a config:CargoMakeCommand ;
    rdfs:label "cargo make bench"@en ;
    config:commandString "cargo make bench" ;
    config:timeoutSeconds 60 ;
    cnv:description "Run performance benchmarks."@en .

config:CargoMakePreCommit a config:CargoMakeCommand ;
    rdfs:label "cargo make pre-commit"@en ;
    config:commandString "cargo make pre-commit" ;
    config:timeoutSeconds 30 ;
    cnv:description "Pre-commit validation: format + lint + unit tests."@en .

# ============================================================================
# ANDON SIGNAL INSTANCES
# ============================================================================

methods:CompilerErrorSignal a methods:AndonCritical ;
    rdfs:label "Compiler Error Signal"@en ;
    cnv:description "Red signal: error[E...] pattern detected. Must stop immediately and fix root cause."@en .

methods:TestFailureSignal a methods:AndonCritical ;
    rdfs:label "Test Failure Signal"@en ;
    cnv:description "Red signal: test ... FAILED pattern detected. Must stop immediately and fix root cause."@en .

methods:CompilerWarningSignal a methods:AndonHigh ;
    rdfs:label "Compiler Warning Signal"@en ;
    cnv:description "Yellow signal: warning: pattern detected. Should stop and fix before proceeding."@en .

methods:LintingErrorSignal a methods:AndonHigh ;
    rdfs:label "Linting Error Signal"@en ;
    cnv:description "Yellow signal: clippy warnings/errors detected. Should stop and fix before proceeding."@en .

# ============================================================================
# PERFORMANCE SLO INSTANCES
# ============================================================================

config:CompilationSLO a config:PerformanceSLO ;
    rdfs:label "Compilation SLO"@en ;
    config:metric "Incremental compilation time" ;
    config:targetValue "≤ 2 seconds" ;
    cnv:description "Incremental builds must complete in 2 seconds or less."@en .

config:UnitTestSLO a config:PerformanceSLO ;
    rdfs:label "Unit Test SLO"@en ;
    config:metric "Unit test execution time" ;
    config:targetValue "≤ 10 seconds" ;
    cnv:description "Unit tests must complete in 10 seconds or less."@en .

config:IntegrationTestSLO a config:PerformanceSLO ;
    rdfs:label "Integration Test SLO"@en ;
    config:metric "Integration test execution time" ;
    config:targetValue "≤ 30 seconds" ;
    cnv:description "Integration tests must complete in 30 seconds or less."@en .

config:CLIExecutionSLO a config:PerformanceSLO ;
    rdfs:label "CLI Execution SLO"@en ;
    config:metric "CLI end-to-end execution time" ;
    config:targetValue "≤ 100 milliseconds" ;
    cnv:description "CLI commands must complete in 100ms or less."@en .

config:MemoryUsageSLO a config:PerformanceSLO ;
    rdfs:label "Memory Usage SLO"@en ;
    config:metric "Peak memory usage" ;
    config:targetValue "≤ 10 MB" ;
    cnv:description "Peak memory usage must not exceed 10 MB."@en .

# ============================================================================
# TEST CATEGORY INSTANCES
# ============================================================================

config:UnitTests a config:TestCategory ;
    rdfs:label "Unit Tests"@en ;
    cnv:description "Colocated with source in src/*_test.rs or #[cfg(test)] mod tests."@en .

config:IntegrationTests a config:TestCategory ;
    rdfs:label "Integration Tests"@en ;
    cnv:description "Located in /tests directory."@en .

config:PropertyTests a config:TestCategory ;
    rdfs:label "Property Tests"@en ;
    cnv:description "Using proptest for command parsing and invariant verification."@en .

config:SnapshotTests a config:TestCategory ;
    rdfs:label "Snapshot Tests"@en ;
    cnv:description "Using insta for deterministic output verification."@en .

# ============================================================================
# SHACL VALIDATION SHAPES
# ============================================================================

config:ProjectShape a sh:NodeShape ;
    sh:targetClass cnv:Project ;
    sh:property [
        sh:path cnv:projectName ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
        sh:message "Project must have exactly one name."@en ;
    ] ;
    sh:property [
        sh:path cnv:hasLanguage ;
        sh:minCount 1 ;
        sh:message "Project must specify at least one language."@en ;
    ] ;
    sh:property [
        sh:path cnv:followsMethodology ;
        sh:minCount 1 ;
        sh:message "Project must follow at least one methodology (SPARC, TDD, DfLSS)."@en ;
    ] .

config:HyperAdvancedAgentShape a sh:NodeShape ;
    sh:targetClass agents:HyperAdvancedAgent ;
    sh:property [
        sh:path agents:agentType ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
        sh:message "Hyper-advanced agent must have exactly one agent type."@en ;
    ] ;
    sh:property [
        sh:path cnv:priority ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:integer ;
        sh:minInclusive 1 ;
        sh:maxInclusive 1 ;
        sh:message "Hyper-advanced agents must have priority 1."@en ;
    ] ;
    sh:property [
        sh:path agents:hasCapability ;
        sh:minCount 3 ;
        sh:message "Hyper-advanced agent must have at least 3 capabilities."@en ;
    ] ;
    sh:property [
        sh:path agents:hasUseCase ;
        sh:minCount 3 ;
        sh:message "Hyper-advanced agent must have at least 3 use cases."@en ;
    ] .

config:AbsoluteRuleShape a sh:NodeShape ;
    sh:targetClass rules:AbsoluteRule ;
    sh:property [
        sh:path cnv:mandatory ;
        sh:hasValue true ;
        sh:message "Absolute rules must be mandatory."@en ;
    ] ;
    sh:property [
        sh:path cnv:description ;
        sh:minCount 1 ;
        sh:datatype xsd:string ;
        sh:message "Absolute rule must have a description."@en ;
    ] .

config:CargoMakeCommandShape a sh:NodeShape ;
    sh:targetClass config:CargoMakeCommand ;
    sh:property [
        sh:path config:commandString ;
        sh:pattern "^cargo make " ;
        sh:message "CargoMakeCommand must start with 'cargo make '."@en ;
    ] ;
    sh:property [
        sh:path config:timeoutSeconds ;
        sh:minCount 1 ;
        sh:datatype xsd:integer ;
        sh:minInclusive 1 ;
        sh:message "CargoMakeCommand must have a positive timeout."@en ;
    ] .

config:PerformanceSLOShape a sh:NodeShape ;
    sh:targetClass config:PerformanceSLO ;
    sh:property [
        sh:path config:metric ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
        sh:message "SLO must specify exactly one metric."@en ;
    ] ;
    sh:property [
        sh:path config:targetValue ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
        sh:message "SLO must specify exactly one target value."@en ;
    ] .

config:AndonSignalShape a sh:NodeShape ;
    sh:targetClass methods:AndonSignal ;
    sh:property [
        sh:path cnv:description ;
        sh:minCount 1 ;
        sh:datatype xsd:string ;
        sh:message "Andon signal must have a description explaining when to stop."@en ;
    ] .

# ============================================================================
# END OF ONTOLOGY
