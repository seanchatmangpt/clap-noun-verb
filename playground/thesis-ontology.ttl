# Hyper-Thesis Framework Ontology
# RDF/Turtle format for oxigraph integration

@prefix : <http://thesis.hyper/framework/> .
@prefix htf: <http://thesis.hyper/framework/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

#################################################################
# 0. Base Ontology Classes
#################################################################

htf:Ontology a rdfs:Class ;
    rdfs:label "Base Ontology" ;
    rdfs:comment "The foundational structure O from which all A derive" .

htf:Architecture a rdfs:Class ;
    rdfs:label "Thesis Architecture" ;
    rdfs:comment "The argument structure A = μ(O)" .

htf:Shard a rdfs:Class ;
    rdfs:label "Δ-Shard" ;
    rdfs:comment "Canonical research component (chapter/section)" .

htf:Order a rdfs:Class ;
    rdfs:label "Λ-Order" ;
    rdfs:comment "Total ordering constraint across shards" .

htf:Merge a rdfs:Class ;
    rdfs:label "Π-Merge" ;
    rdfs:comment "Composition operator for shard fusion" .

htf:Globalization a rdfs:Class ;
    rdfs:label "Γ-Globalization" ;
    rdfs:comment "Gluing operator ensuring coherence" .

htf:Invariant a rdfs:Class ;
    rdfs:label "Q-Invariant" ;
    rdfs:comment "Property preserved across all layers" .

htf:Evolution a rdfs:Class ;
    rdfs:label "τ-Evolution" ;
    rdfs:comment "Temporal progression toward fixed point" .

#################################################################
# 1. Δ-Shard Families (Seven Canonical Types)
#################################################################

# IMRaD Family
htf:IMRaDFamily a rdfs:Class ;
    rdfs:label "IMRaD Family" ;
    rdfs:subClassOf htf:Shard .

htf:Introduction a htf:IMRaDFamily ;
    htf:position 1 ;
    htf:purpose "Establish context, motivation, research questions" .

htf:Method a htf:IMRaDFamily ;
    htf:position 2 ;
    htf:purpose "Describe methodology, design, implementation" .

htf:Results a htf:IMRaDFamily ;
    htf:position 3 ;
    htf:purpose "Present findings, experimental validation" .

htf:Discussion a htf:IMRaDFamily ;
    htf:position 4 ;
    htf:purpose "Interpret results, discuss implications" .

# Thesis-by-Papers Family
htf:PaperFamily a rdfs:Class ;
    rdfs:label "Thesis by Papers Family" ;
    rdfs:subClassOf htf:Shard .

htf:PaperOne a htf:PaperFamily ;
    htf:purpose "Foundational contribution" ;
    htf:minLength 8000 .

htf:PaperTwo a htf:PaperFamily ;
    htf:purpose "Major advance or application" ;
    htf:minLength 8000 .

htf:PaperThree a htf:PaperFamily ;
    htf:purpose "Novel methodology or theory" ;
    htf:minLength 8000 .

htf:Synthesis a htf:PaperFamily ;
    htf:purpose "Unify papers into coherent narrative" ;
    htf:minLength 5000 .

# Argument Family
htf:ArgumentFamily a rdfs:Class ;
    rdfs:label "Argument Family" ;
    rdfs:subClassOf htf:Shard .

htf:Claim a htf:ArgumentFamily ;
    htf:purpose "State central thesis claim" ;
    htf:required "true"^^xsd:boolean .

htf:Ground a htf:ArgumentFamily ;
    htf:purpose "Provide evidence/justification" ;
    htf:required "true"^^xsd:boolean .

htf:Proof a htf:ArgumentFamily ;
    htf:purpose "Formal or rigorous demonstration" ;
    htf:required "true"^^xsd:boolean .

htf:Objection a htf:ArgumentFamily ;
    htf:purpose "Raise counter-argument or limitation" ;
    htf:required "false"^^xsd:boolean .

htf:Reply a htf:ArgumentFamily ;
    htf:purpose "Respond to objections" ;
    htf:required "false"^^xsd:boolean .

# Contribution Family
htf:ContributionFamily a rdfs:Class ;
    rdfs:label "Contribution Family" ;
    rdfs:subClassOf htf:Shard .

htf:Gap a htf:ContributionFamily ;
    htf:purpose "Identify research gap/problem" ;
    htf:required "true"^^xsd:boolean .

htf:Design a htf:ContributionFamily ;
    htf:purpose "Propose solution/design" ;
    htf:required "true"^^xsd:boolean .

htf:Evaluation a htf:ContributionFamily ;
    htf:purpose "Evaluate contribution validity" ;
    htf:required "true"^^xsd:boolean .

htf:Impact a htf:ContributionFamily ;
    htf:purpose "Demonstrate real-world impact" ;
    htf:required "false"^^xsd:boolean .

# Monograph Family
htf:MonographFamily a rdfs:Class ;
    rdfs:label "Monograph Family" ;
    rdfs:subClassOf htf:Shard .

htf:Context a htf:MonographFamily ;
    htf:purpose "Deep background and literature review" .

htf:Canon a htf:MonographFamily ;
    htf:purpose "Establish canonical knowledge base" .

htf:MethodMono a htf:MonographFamily ;
    htf:purpose "Detailed methodology exposition" .

htf:Analysis a htf:MonographFamily ;
    htf:purpose "Comprehensive analysis and synthesis" .

htf:Conclusion a htf:MonographFamily ;
    htf:purpose "Final synthesis and implications" .

# DSR (Design Science Research) Family
htf:DSRFamily a rdfs:Class ;
    rdfs:label "DSR Family" ;
    rdfs:subClassOf htf:Shard .

htf:Problem a htf:DSRFamily ;
    htf:purpose "Define problem precisely" ;
    htf:required "true"^^xsd:boolean .

htf:Artifact a htf:DSRFamily ;
    htf:purpose "Design and build artifact" ;
    htf:required "true"^^xsd:boolean .

htf:EvaluationDSR a htf:DSRFamily ;
    htf:purpose "Evaluate artifact effectiveness" ;
    htf:required "true"^^xsd:boolean .

htf:Theory a htf:DSRFamily ;
    htf:purpose "Extract generalizable theory" ;
    htf:required "false"^^xsd:boolean .

# Narrative Family
htf:NarrativeFamily a rdfs:Class ;
    rdfs:label "Narrative Family" ;
    rdfs:subClassOf htf:Shard .

htf:Field a htf:NarrativeFamily ;
    htf:purpose "Establish field and historical context" .

htf:Voice a htf:NarrativeFamily ;
    htf:purpose "Develop authorial voice and perspective" .

htf:Pattern a htf:NarrativeFamily ;
    htf:purpose "Identify recurring patterns and themes" .

htf:Insight a htf:NarrativeFamily ;
    htf:purpose "Distill key insights and wisdom" .

#################################################################
# 2. Λ-Total Order Relations
#################################################################

htf:precedes a rdf:Property ;
    rdfs:label "precedes" ;
    rdfs:comment "Defines ordering constraint A Λ:≺ B" ;
    rdf:type rdf:Property .

htf:Problem htf:precedes htf:Gap .
htf:Gap htf:precedes htf:Claim .
htf:Claim htf:precedes htf:Introduction .
htf:Introduction htf:precedes htf:Method .
htf:Method htf:precedes htf:MethodMono .
htf:MethodMono htf:precedes htf:Voice .
htf:Voice htf:precedes htf:Canon .
htf:Canon htf:precedes htf:Field .
htf:Field htf:precedes htf:Artifact .
htf:Artifact htf:precedes htf:Proof .
htf:Proof htf:precedes htf:PaperOne .
htf:PaperOne htf:precedes htf:Results .
htf:Results htf:precedes htf:PaperTwo .
htf:PaperTwo htf:precedes htf:Evaluation .
htf:Evaluation htf:precedes htf:EvaluationDSR .
htf:EvaluationDSR htf:precedes htf:Objection .
htf:Objection htf:precedes htf:Discussion .
htf:Discussion htf:precedes htf:Reply .
htf:Reply htf:precedes htf:Pattern .
htf:Pattern htf:precedes htf:Theory .
htf:Theory htf:precedes htf:Analysis .
htf:Analysis htf:precedes htf:Synthesis .
htf:Synthesis htf:precedes htf:Insight .
htf:Insight htf:precedes htf:Impact .
htf:Impact htf:precedes htf:Conclusion .

#################################################################
# 3. Π-Merge Operations
#################################################################

htf:mergeableWith a rdf:Property ;
    rdfs:label "mergeable with" ;
    rdfs:comment "Defines which shards can be composed (Π:⊕)" .

htf:PaperOne htf:mergeableWith htf:PaperTwo .
htf:PaperTwo htf:mergeableWith htf:PaperThree .
htf:PaperThree htf:mergeableWith htf:Synthesis .

htf:Problem htf:mergeableWith htf:Gap .
htf:Gap htf:mergeableWith htf:Claim .
htf:Design htf:mergeableWith htf:Artifact .

#################################################################
# 4. Q-Invariants (Properties that must hold everywhere)
#################################################################

htf:Coherence a htf:Invariant ;
    rdfs:label "Coherence Invariant" ;
    rdfs:comment "All shards must align with central claim" ;
    htf:scope "global" .

htf:Completeness a htf:Invariant ;
    rdfs:label "Completeness Invariant" ;
    rdfs:comment "All required shards must be present" ;
    htf:scope "global" .

htf:Evidence a htf:Invariant ;
    rdfs:label "Evidence Invariant" ;
    rdfs:comment "All claims must have supporting evidence" ;
    htf:scope "local" .

htf:Logicality a htf:Invariant ;
    rdfs:label "Logicality Invariant" ;
    rdfs:comment "Argument chain must be logically sound" ;
    htf:scope "global" .

htf:Novelty a htf:Invariant ;
    rdfs:label "Novelty Invariant" ;
    rdfs:comment "Contribution must advance the field" ;
    htf:scope "global" .

htf:Clarity a htf:Invariant ;
    rdfs:label "Clarity Invariant" ;
    rdfs:comment "All sections must be understandable" ;
    htf:scope "local" .

htf:Integration a htf:Invariant ;
    rdfs:label "Integration Invariant" ;
    rdfs:comment "All sections must integrate into whole" ;
    htf:scope "global" .

#################################################################
# 5. Shard Metadata Schema
#################################################################

htf:shardName a rdf:Property ;
    rdfs:domain htf:Shard ;
    rdfs:range xsd:string .

htf:shardFamily a rdf:Property ;
    rdfs:domain htf:Shard ;
    rdfs:range htf:Shard .

htf:wordCount a rdf:Property ;
    rdfs:domain htf:Shard ;
    rdfs:range xsd:integer .

htf:status a rdf:Property ;
    rdfs:domain htf:Shard ;
    rdfs:range xsd:string .

htf:priority a rdf:Property ;
    rdfs:domain htf:Shard ;
    rdfs:range xsd:integer .

htf:evidence a rdf:Property ;
    rdfs:domain htf:Shard ;
    rdfs:range rdf:Seq .

htf:relatesTo a rdf:Property ;
    rdfs:domain htf:Shard ;
    rdfs:range htf:Shard .

#################################################################
# 6. Example: PhD Thesis Instance
#################################################################

htf:TrillionAgentThesis a htf:Architecture ;
    htf:title "Orchestrated Trillion-Agent Ecosystems: 2028 Revolution" ;
    htf:author "Claude Code" ;
    htf:year 2025 ;
    htf:centralClaim htf:TrillionAgentClaim ;
    htf:wordCountTarget 20000 .

htf:TrillionAgentClaim a htf:Claim ;
    htf:statement "Trillion-agent ecosystems will revolutionize 6 domains by 2028" ;
    htf:confidence "high" .

# Map shards to PhD thesis
htf:TrillionAgentThesis htf:hasShard htf:PhDIntro .
htf:TrillionAgentThesis htf:hasShard htf:PhDMethod .
htf:TrillionAgentThesis htf:hasShard htf:PhDResults .
htf:TrillionAgentThesis htf:hasShard htf:PhDDiscuss .

htf:PhDIntro a htf:Introduction ;
    htf:status "complete" ;
    htf:wordCount 3200 ;
    htf:priority 1 .

htf:PhDMethod a htf:Method ;
    htf:status "complete" ;
    htf:wordCount 5100 ;
    htf:priority 2 .

htf:PhDResults a htf:Results ;
    htf:status "complete" ;
    htf:wordCount 4800 ;
    htf:priority 3 .

htf:PhDDiscuss a htf:Discussion ;
    htf:status "complete" ;
    htf:wordCount 4900 ;
    htf:priority 4 .

#################################################################
